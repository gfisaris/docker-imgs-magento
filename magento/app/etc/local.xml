<config>
    <global>
        <install>
            <date>mymgt-date</date>
        </install>
        <crypt>
            <key>mymgt-key</key>
        </crypt>
        <disable_local_modules>false</disable_local_modules>
        <resources>
            <db>
                <table_prefix>mydb-prefix</table_prefix>
            </db>
            <default_setup>
                <connection>
                    <host>mydb-host.com</host>
                    <username>mydb-user</username>
                    <password>mydb-pass</password>
                    <dbname>mydb-name</dbname>
                    <initStatements>mydb-init_stm</initStatements>
                    <model>mydb-model</model>
                    <type>mydb-type</type>
                    <pdoType>mydb-pod_type</pdoType>
                    <active>1</active>
                </connection>
            </default_setup>
        </resources>
        <session_save>mymgt-sessions</session_save>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <frontName>mymgt-admin_url</frontName>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
